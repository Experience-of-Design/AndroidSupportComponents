<enum-method-mappings>
	<!--
  This example changes the Java method:
    android.support.v4.app.Fragment.SavedState.writeToParcel (int flags)
  to be:
    android.support.v4.app.Fragment.SavedState.writeToParcel (Android.OS.ParcelableWriteFlags flags)
  when bound in C#.

  <mapping jni-class="android/support/v4/app/Fragment.SavedState">
    <method jni-name="writeToParcel" parameter="flags" clr-enum-type="Android.OS.ParcelableWriteFlags" />
  </mapping>

  Notes:
  - For Java interfaces, use "jni-interface" attribute instead of "jni-class" attribute.
  - To change the type of the return value, use "return" as the parameter name.
  - The parameter names will be p0, p1, ... unless you provide JavaDoc file in the project.
  -->
	<mapping jni-class="android/support/graphics/drawable/VectorDrawableCommon">
		<method jni-name="getLayoutDirection" parameter="return" clr-enum-type="Android.Graphics.Drawables.Drawable.LayoutDirection" />
		<method jni-name="setChangingConfigurations" parameter="configs" clr-enum-type="Android.Content.PM.ConfigChanges" />
	</mapping>

	<mapping jni-class="android/support/graphics/drawable/VectorDrawableCompat">
		<method jni-name="getLayoutDirection" parameter="return" clr-enum-type="Android.Graphics.Drawables.Drawable.LayoutDirection" />
	</mapping>
</enum-method-mappings>
